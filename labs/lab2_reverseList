def reverse_list(lst, low=None, high=None):
    """
    :param lst: list[]
    :param low: int
    :param high: int
    :return: None
    """
    if low == None:
        low = 0
    if high == None:
        high = len(lst) - 1
    while low < high:
        lst[low], lst[high] = lst[high], lst[low]
        low += 1
        high -= 1
    return lst


print(reverse_list([1, 2, 3, 4, 5, 6]))

print(reverse_list([1, 2, 3, 4, 5, 6], 1, 3))
